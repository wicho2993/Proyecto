{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest"
        ]
      }
    },
    {
      "name": "files",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "/**/*.css",
          "/**/*.js"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-data",
      "urls": [
        "https://api.example.com/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1h",
        "timeToLive": "1h"
      }
    }
  ]
}
